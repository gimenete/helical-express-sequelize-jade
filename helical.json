{
  "generators": [
    {
      "source": "generators/dotenv",
      "path": ".env"
    },
    {
      "source": "generators/gitignore",
      "path": ".gitignore"
    },
    {
      "source": "generators/models.js",
      "path": "models/index.js"
    },
    {
      "source": "generators/package.json",
      "path": "package.json"
    },
    {
      "source": "generators/auth-twitter.js",
      "path": "controllers/auth/twitter.js"
    },
    {
      "source": "generators/auth-facebook.js",
      "path": "controllers/auth/facebook.js"
    },
    {
      "source": "generators/auth.js",
      "path": "controllers/auth/auth.js"
    },
    {
      "source": "generators/home.js",
      "path": "controllers/home.js"
    },
    {
      "source": "generators/layout.jade",
      "path": "views/layout.jade"
    },
    {
      "source": "generators/index.jade",
      "path": "views/index.jade"
    },
    {
      "source": "generators/api-controller.js",
      "path": "controllers/api/{{ object.name | lower }}.js",
      "foreach": "entities"
    },
    {
      "source": "generators/app.js",
      "path": "app.js"
    },
    {
      "source": "generators/admin-home.js",
      "path": "controllers/admin/home.js"
    },
    {
      "source": "generators/admin-controller.js",
      "path": "controllers/admin/{{ object.name | lower }}.js",
      "foreach": "entities"
    },
    {
      "source": "generators/layout-admin.jade",
      "path": "views/admin/layout.jade"
    },
    {
      "source": "generators/admin.jade",
      "path": "views/admin/index.jade"
    },
    {
      "source": "generators/pagination.js",
      "path": "controllers/pagination.js"
    },
    {
      "source": "generators/pagination.jade",
      "path": "views/admin/pagination.jade"
    },
    {
      "source": "generators/entity-edit.jade",
      "path": "views/admin/{{ object.name | lower }}/edit.jade",
      "foreach": "entities"
    },
    {
      "source": "generators/entity-list.jade",
      "path": "views/admin/{{ object.name | lower }}/list.jade",
      "foreach": "entities"
    }
  ],
  "options": [
    {
      "name": "auth",
      "describe": "Authentication mechanism",
      "type": "string",
      "choices": ["twitter", "facebook", ""],
      "default": ""
    }
  ]
}
